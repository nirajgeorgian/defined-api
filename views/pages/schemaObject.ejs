<% include ../partials/_header %>
<% include ../partials/_navbar.ejs %>

  <div class="row justify-content-center">
    <div class="col-sm-6 col-md-6">
      <h4 class="text-center">You have defined your schema name as <small class="text-muted"><%= name %></small></h4>
      <hr>
      <p class="text-center">For defined schema following collections are available : <small class="text-muted">Choose one to get started</small></p>
      <% schemaData.forEach(function(data) { %>
        <div class="row justify-content-center">
          <div class="col-sm-10 col-md-10 col-lg-10">
            <label for="<%- data._id %>">Link: </label>
            <a href="/client/schema/<%- data.schema %>/<%- data._id %>" class="btn btn-success" id='<%- data._id %>'><%- data._id %></a>
          </div>
        </div>
        <hr>
      <% }) %>
    </div>
  </div>

<% include ../partials/_footer %>
